{
  "name": "saas-sub-frontend",
  "version": "0.1.0",
  "author": "wbx",
  "scripts": {
    "etcd-prod": "ETCD_ENV=production node ./bin/etcd.js",
    "etcd-sandbox": "ETCD_ENV=sandbox node ./bin/etcd.js",
    "pm2-docker-prod": "npm-run-all --sequential compile etcd-prod start-pm2",
    "pm2-docker-sandbox": "npm-run-all --sequential compile etcd-sandbox start-pm2",
    "pm2": "npm-run-all --sequential compile start-pm2",
    "start-pm2": "pm2 start pm2.json --no-daemon",
    "start": "npm-run-all --sequential compile start-prod",
    "start-prod": "NODE_ENV=production node ./bin/production",
    "dev": "npm-run-all --sequential clean start-dev",
    "start-dev": "NODE_ENV=development nodemon --ignore webpack-assets.json --ignore client --ignore client-src --exec  babel-node ./bin/development.js",
    "build": "npm test && npm run compile",
    "compile": "npm-run-all clean --parallel compile-server compile-client",
    "compile-server": "babel server-src/ --out-dir server/ --retain-lines --source-maps",
    "compile-client": "NODE_ENV=production webpack -p --colors --config ./webpack.config.js",
    "clean": "rimraf server && rimraf client/js/pages",
    "test": "NODE_ENV=test nyc --reporter=lcov ava --verbose && nyc report",
    "test1": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "engines": {
    "node": ">= 4"
  },
  "ava": {
    "babel": {
      "presets": [
        "es2015",
        "stage-3"
      ]
    },
    "files": [
      "test/*.js"
    ],
    "failFast": true,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "pligins": [
      "espower",
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  },
