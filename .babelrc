{
  "presets": ["es2015", "stage-3"],
  "plugins": [
  		"add-module-exports", 
  		"transform-runtime", 
  		"transform-decorators-legacy", 
  		"transform-object-rest-spread",
        ["babel-plugin-transform-builtin-extend", {
            globals: ["Error", "Array"]
        }]
    ],
  "sourceMaps": true,
  "retainLines": true
}
